<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.madproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MADproject"
        tools:targetApi="31">
        <activity
            android:name=".weather_condition"
            android:exported="false" />
        <activity
            android:name=".feedback_home"
            android:exported="false" />
        <activity
            android:name=".create_feedback"
            android:exported="false" />
        <activity
            android:name=".Update_feedback"
            android:exported="false" />
        <activity
            android:name=".view_feedbacks"
            android:exported="false" />
        <activity
            android:name=".HealthTracker"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>